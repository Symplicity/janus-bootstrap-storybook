{
  "name": "janus-bootstrap-storybook",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Symplicity/janus-bootstrap-storybook.git"
  },
  "description": "Storybook to build, test, certify, and document reference components with Janus-Bootstrap",
  "keywords": [
    "symplicity",
    "design system",
    "janus",
    "booststrap",
    "storybook",
    "html",
    "rtl"
  ],
  "license": "MIT",
  "author": "",
  "main": "index.js",
  "scripts": {
    "build-storybook": "build-storybook",
    "storybook": "start-storybook -p 9009",
    "test": "npm-run-all test:*",
    "test:run": "docker run --rm -v $(pwd):/src backstopjs/backstopjs:6.0.4 test",
    "test:view-report": "open-cli backstop_data/html_report/index.html",
    "approve": "docker run --rm -v $(pwd):/src backstopjs/backstopjs:6.0.4 approve",
    "reference": "docker run --rm -v $(pwd):/src backstopjs/backstopjs:6.0.4 reference"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/plugin-proposal-private-property-in-object": "^7.14.5",
    "@storybook/addon-a11y": "^6.4.18",
    "@storybook/addon-actions": "^6.4.18",
    "@storybook/addon-backgrounds": "^6.4.18",
    "@storybook/addon-essentials": "^6.4.18",
    "@storybook/addon-events": "^6.2.9",
    "@storybook/addon-knobs": "^6.4.0",
    "@storybook/addon-links": "^6.4.18",
    "@storybook/addon-notes": "^5.3.21",
    "@storybook/addon-options": "^5.3.21",
    "@storybook/addon-storysource": "^6.4.18",
    "@storybook/addon-viewport": "^6.4.18",
    "@storybook/addons": "^6.4.18",
    "@storybook/client-api": "^6.4.18",
    "@storybook/core": "^6.4.18",
    "@storybook/core-events": "^6.4.18",
    "@storybook/html": "^6.4.18",
    "@symplicity/bootstrap": "github:Symplicity/bootstrap#v5.1.3-janus",
    "@symplicity/janus-tokens": "github:Symplicity/janus-tokens",
    "@symplicity/tabler-icons": "github:Symplicity/tabler-icons",
    "babel-loader": "^8.2.2",
    "expose-loader": "^0.7.5",
    "jquery": "^3.6.0",
    "npm-run-all": "^4.1.5",
    "open-cli": "^6.0.1",
    "raw-loader": "^4.0.2",
    "sass-loader": "^8.0.0",
    "storybook-addon-rtl": "^0.4.2"
  },
  "dependencies": {
    "@storybook/theming": "^6.4.18",
    "@tabler/icons": "github:Symplicity/tabler-icons",
    "bootstrap": "github:Symplicity/bootstrap#v5.0.2-janus"
  }
}
