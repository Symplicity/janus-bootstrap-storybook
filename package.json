{
  "name": "janus-bootstrap-storybook",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Symplicity/janus-bootstrap-storybook.git"
  },
  "description": "Storybook to build, test, certify, and document reference components with Janus-Bootstrap",
  "keywords": [
    "symplicity",
    "design system",
    "janus",
    "booststrap",
    "storybook",
    "html",
    "rtl"
  ],
  "license": "MIT",
  "author": "",
  "main": "index.js",
  "scripts": {
    "build-storybook": "build-storybook",
    "storybook": "start-storybook -p 9009 -s public",
    "test": "npm-run-all test:*",
    "test:run": "docker run --rm -v $(pwd):/src backstopjs/backstopjs test",
    "test:view-report": "open-cli backstop_data/html_report/index.html",
    "approve": "docker run --rm -v $(pwd):/src backstopjs/backstopjs approve",
    "reference": "docker run --rm -v $(pwd):/src backstopjs/backstopjs reference"
  },
  "devDependencies": {
    "@babel/core": "^7.13.15",
    "@storybook/addon-a11y": "^6.2.8",
    "@storybook/addon-actions": "^6.2.8",
    "@storybook/addon-backgrounds": "^6.2.8",
    "@storybook/addon-essentials": "^6.2.8",
    "@storybook/addon-events": "^6.2.8",
    "@storybook/addon-knobs": "^6.2.8",
    "@storybook/addon-links": "^6.2.8",
    "@storybook/addon-notes": "^5.3.21",
    "@storybook/addon-options": "^5.3.21",
    "@storybook/addon-storysource": "^6.2.8",
    "@storybook/addon-viewport": "^6.2.8",
    "@storybook/addons": "^6.2.8",
    "@storybook/client-api": "^6.2.8",
    "@storybook/core": "^6.2.8",
    "@storybook/core-events": "^6.2.8",
    "@storybook/html": "^6.2.8",
    "@symplicity/bootstrap": "git+https://github.com/Symplicity/bootstrap.git#v5.0.0-beta3-janus",
    "@symplicity/janus-tokens": "git+https://github.com/Symplicity/janus-tokens.git",
    "@symplicity/tabler-icons": "git+ssh://git@github.com/Symplicity/tabler-icons.git",
    "babel-loader": "^8.2.2",
    "expose-loader": "^0.7.5",
    "jquery": "^3.6.0",
    "npm-run-all": "^4.1.5",
    "open-cli": "^6.0.1",
    "raw-loader": "^4.0.2",
    "sass-loader": "^8.0.0",
    "storybook-addon-rtl": "^0.2.2"
  },
  "dependencies": {
    "@storybook/theming": "^6.2.8",
    "@symplicity/tabler-icons": "git+ssh://git@github.com/Symplicity/tabler-icons.git",
    "bootstrap": "git+https://github.com/Symplicity/bootstrap.git#v5.0.0-beta1-janus"
  }
}
